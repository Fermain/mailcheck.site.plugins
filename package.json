{
    "name": "mailcheck",
    "version": "2.0.0",
    "description": "An application to clean your subscription list from bounces, and enrich data about customers.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "webpack-dev-server --config webpack.dev.js --mode development",
        "build": "webpack --config webpack.prod.js --mode production",
        "lint": "eslint . *.js",
        "lint-css": "stylelint \"src/scss/**/*.+(scss|css)\" --fix",
        "preview": "npm run build && http-server dist",
        "deploy_hosting": "firebase deploy --only hosting --project=dev",
        "deploy_functions": "firebase deploy --only functions --project=dev",
        "deploy_hosting:prod": "firebase deploy --only hosting --project=prod",
        "deploy_functions:prod": "firebase deploy --only functions --project=prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lifenautjoe/webpack-starter-basic.git"
    },
    "keywords": [
        "webpack4",
        "webpack",
        "starter",
        "starter-kit",
        "kit",
        "webpack",
        "seed",
        "webpack-seed",
        "webpack4-seed",
        "webpack4-boilerplate",
        "boilerplate",
        "javascript",
        "js",
        "es6",
        "es6-starter",
        "js-starter",
        "es6-starter-kit"
    ],
    "author": "Joel Hernandez",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lifenautjoe/webpack-starter-basic/issues"
    },
    "homepage": "https://github.com/lifenautjoe/webpack-starter-basic#readme",
    "dependencies": {
        "normalize.css": "^8.0.1"
    },
    "devDependencies": {
        "@nodeart/stylelint-config-nodeart": "^0.0.4",
        "ajv": "^6.3.0",
        "autoprefixer": "^9.8.6",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^0.28.11",
        "cssnano": "^3.10.0",
        "duplicate-package-checker-webpack-plugin": "^3.0.0",
        "eslint": "^7.6.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "favicons-webpack-plugin": "0.0.9",
        "file-loader": "^1.1.11",
        "firebase-tools": "^8.7.0",
        "handlebars": "^4.7.6",
        "handlebars-loader": "^1.7.1",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "inquirer": "^5.1.0",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^2.1.6",
        "rimraf": "^2.7.1",
        "sass-loader": "^6.0.7",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.20.3",
        "stylelint": "^13.7.2",
        "svg-url-loader": "^6.0.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.1.1"
    },
    "stylelint": {
        "extends": "@nodeart/stylelint-config-nodeart",
        "ignoreFiles": [
            "src/scss/libs/*.+(scss|css)"
        ],
        "rules": {
            "no-descending-specificity": null,
            "plugin/no-low-performance-animation-properties": null,
            "sh-waqar/declaration-use-variable": null
        }
    },
    "browserslist": [
        "defaults"
    ]
}
